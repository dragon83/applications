- model_class = UserProfile
.page-header
  %h1=t '.title', :default => model_class.model_name.human.pluralize.titleize
%table.table.table-striped
  %thead
    %tr
      %th= model_class.human_attribute_name(:id)
      %th= model_class.human_attribute_name(:user_id)
      %th= model_class.human_attribute_name(:gender)
      %th= model_class.human_attribute_name(:dob)
      %th= model_class.human_attribute_name(:tob)
      %th= model_class.human_attribute_name(:pob)
      %th= model_class.human_attribute_name(:age)
      %th= model_class.human_attribute_name(:salary)
      %th= model_class.human_attribute_name(:caste_id)
      %th= model_class.human_attribute_name(:sub_caste_id)
      %th= model_class.human_attribute_name(:height)
      %th= model_class.human_attribute_name(:religion_id)
      %th= model_class.human_attribute_name(:affluence_id)
      %th= model_class.human_attribute_name(:profession)
      %th= model_class.human_attribute_name(:education)
      %th= model_class.human_attribute_name(:work_city)
      %th= model_class.human_attribute_name(:work_state)
      %th= model_class.human_attribute_name(:food_habit)
      %th= model_class.human_attribute_name(:mangalik)
      %th= model_class.human_attribute_name(:special_status)
      %th= model_class.human_attribute_name(:specs)
      %th= model_class.human_attribute_name(:spec_power)
      %th= model_class.human_attribute_name(:marital_status)
      %th= model_class.human_attribute_name(:mother_tongue)
      %th= model_class.human_attribute_name(:complexion)
      %th= model_class.human_attribute_name(:body_type)
      %th= model_class.human_attribute_name(:weight)
      %th= model_class.human_attribute_name(:father_status)
      %th= model_class.human_attribute_name(:mother_status)
      %th= model_class.human_attribute_name(:father_profession)
      %th= model_class.human_attribute_name(:mother_profession)
      %th= model_class.human_attribute_name(:native)
      %th= model_class.human_attribute_name(:family_values)
      %th= model_class.human_attribute_name(:elder_brothers)
      %th= model_class.human_attribute_name(:elder_sisters)
      %th= model_class.human_attribute_name(:younger_brothers)
      %th= model_class.human_attribute_name(:younger_sisters)
      %th= model_class.human_attribute_name(:created_at)
      %th=t '.actions', :default => t("helpers.actions")
  %tbody
    - @user_profiles.each do |user_profile|
      %tr
        %td= link_to user_profile.id, user_profile_path(user_profile)
        %td= user_profile.user_id
        %td= user_profile.gender
        %td= user_profile.dob
        %td= user_profile.tob
        %td= user_profile.pob
        %td= user_profile.age
        %td= user_profile.salary
        %td= user_profile.caste_id
        %td= user_profile.sub_caste_id
        %td= user_profile.height
        %td= user_profile.religion_id
        %td= user_profile.affluence_id
        %td= user_profile.profession
        %td= user_profile.education
        %td= user_profile.work_city
        %td= user_profile.work_state
        %td= user_profile.food_habit
        %td= user_profile.mangalik
        %td= user_profile.special_status
        %td= user_profile.specs
        %td= user_profile.spec_power
        %td= user_profile.marital_status
        %td= user_profile.mother_tongue
        %td= user_profile.complexion
        %td= user_profile.body_type
        %td= user_profile.weight
        %td= user_profile.father_status
        %td= user_profile.mother_status
        %td= user_profile.father_profession
        %td= user_profile.mother_profession
        %td= user_profile.native
        %td= user_profile.family_values
        %td= user_profile.elder_brothers
        %td= user_profile.elder_sisters
        %td= user_profile.younger_brothers
        %td= user_profile.younger_sisters
        %td=l user_profile.created_at
        %td
          = link_to t('.edit', :default => t("helpers.links.edit")), edit_user_profile_path(user_profile), :class => 'btn btn-mini'
          = link_to t('.destroy', :default => t("helpers.links.destroy")), user_profile_path(user_profile), :method => :delete, :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-mini btn-danger'

= link_to t('.new', :default => t("helpers.links.new")), new_user_profile_path, :class => 'btn btn-primary'
